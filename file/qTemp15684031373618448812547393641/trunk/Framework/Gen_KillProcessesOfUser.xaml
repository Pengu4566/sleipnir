  <Sequence sap2010:Annotation.AnnotationText="Kills all the processes of associated with the names in input array. Kills only the processes running under username specified in the arguments. Catches any error.&#xA;&#xA;Arguments:&#xA;(IN): str_username - Username of robot. Defaults to current username.&#xA;(IN) str_ar_processNames - Names of the processes to be killed." DisplayName="Gen_KillProcessesOfUser" sap2010:WorkflowViewState.IdRef="Sequence_1">
    <ui:LogMessage DisplayName="Log message" sap2010:WorkflowViewState.IdRef="LogMessage_5" Level="Info" Message="[&quot;[INVOKE.START] Trying to kill the following processes:  &quot; + STring.Join(&quot;,&quot;, str_ar_processNames)]" />
    <ui:ForEach x:TypeArguments="x:String" CurrentIndex="{x:Null}" DisplayName="For each process_name" sap2010:WorkflowViewState.IdRef="ForEach`1_2" Values="[str_ar_processNames]">
      <ui:ForEach.Body>
        <ActivityAction x:TypeArguments="x:String">
          <ActivityAction.Argument>
            <DelegateInArgument x:TypeArguments="x:String" Name="process_name" />
          </ActivityAction.Argument>
          <Sequence DisplayName="Body" sap2010:WorkflowViewState.IdRef="Sequence_2">
            <Sequence.Variables>
              <Variable x:TypeArguments="sd:Process[]" Name="process_list" />
              <Variable x:TypeArguments="x:Int32" Name="int_processCounter" />
            </Sequence.Variables>
            <Assign DisplayName="Get Processes by name" sap2010:WorkflowViewState.IdRef="Assign_1">
              <Assign.To>
                <OutArgument x:TypeArguments="sd:Process[]">[process_list]</OutArgument>
              </Assign.To>
              <Assign.Value>
                <InArgument x:TypeArguments="sd:Process[]">[System.Diagnostics.Process.GetProcessesByName(process_name.Trim)]</InArgument>
              </Assign.Value>
            </Assign>
            <Assign DisplayName="Set the process counter" sap2010:WorkflowViewState.IdRef="Assign_2">
              <Assign.To>
                <OutArgument x:TypeArguments="x:Int32">[int_processCounter]</OutArgument>
              </Assign.To>
              <Assign.Value>
                <InArgument x:TypeArguments="x:Int32">1</InArgument>
              </Assign.Value>
            </Assign>
            <ui:ForEach x:TypeArguments="sd:Process" CurrentIndex="{x:Null}" DisplayName="For each active process" sap2010:WorkflowViewState.IdRef="ForEach`1_4" Values="[process_list]">
              <ui:ForEach.Body>
                <ActivityAction x:TypeArguments="sd:Process">
                  <ActivityAction.Argument>
                    <DelegateInArgument x:TypeArguments="sd:Process" Name="process" />
                  </ActivityAction.Argument>
                  <If Condition="[process.StartInfo.Environment(&quot;USERNAME&quot;).Equals(str_username)]" DisplayName="If username is chosen user" sap2010:WorkflowViewState.IdRef="If_1">
                    <If.Then>
                      <Sequence DisplayName="Kill process of chosen user" sap2010:WorkflowViewState.IdRef="Sequence_4">
                        <ui:LogMessage DisplayName="Log message" sap2010:WorkflowViewState.IdRef="LogMessage_1" Level="Info" Message="[&quot;[PROCESS.KILL] &quot; + process_name + &quot; (&quot; +  int_processCounter.ToString +&quot;)&quot;]" />
                        <TryCatch DisplayName="Try to kill process" sap2010:WorkflowViewState.IdRef="TryCatch_1">
                          <TryCatch.Try>
                            <Sequence DisplayName="Kill Process" sap2010:WorkflowViewState.IdRef="Sequence_6">
                              <ui:KillProcess ContinueOnError="{x:Null}" ProcessName="{x:Null}" DisplayName="Kill process" sap2010:WorkflowViewState.IdRef="KillProcess_1" Process="[process]" />
                              <Assign DisplayName="Increment process counter" sap2010:WorkflowViewState.IdRef="Assign_3">
                                <Assign.To>
                                  <OutArgument x:TypeArguments="x:Int32">[int_processCounter]</OutArgument>
                                </Assign.To>
                                <Assign.Value>
                                  <InArgument x:TypeArguments="x:Int32">[int_processCounter + 1]</InArgument>
                                </Assign.Value>
                              </Assign>
                            </Sequence>
                          </TryCatch.Try>
                          <TryCatch.Catches>
                            <Catch x:TypeArguments="s:InvalidOperationException" sap2010:WorkflowViewState.IdRef="Catch`1_1">
                              <ActivityAction x:TypeArguments="s:InvalidOperationException">
                                <ActivityAction.Argument>
                                  <DelegateInArgument x:TypeArguments="s:InvalidOperationException" Name="exception" />
                                </ActivityAction.Argument>
                                <ui:LogMessage DisplayName="Log message" sap2010:WorkflowViewState.IdRef="LogMessage_2" Level="Warn" Message="[&quot;[PROCESS.KILL] Failed to kill process &quot; + process_name + &quot;  most likely because the process has already exited. &quot; + exception.Message]" />
                              </ActivityAction>
                            </Catch>
                            <Catch x:TypeArguments="sc:Win32Exception" sap2010:WorkflowViewState.IdRef="Catch`1_2">
                              <ActivityAction x:TypeArguments="sc:Win32Exception">
                                <ActivityAction.Argument>
                                  <DelegateInArgument x:TypeArguments="sc:Win32Exception" Name="exception" />
                                </ActivityAction.Argument>
                                <ui:LogMessage DisplayName="Log message" sap2010:WorkflowViewState.IdRef="LogMessage_3" Level="Warn" Message="[&quot;[PROCESS.KILL] Failed to kill process &quot; + process_name + &quot; &quot; + exception.Message]" />
                              </ActivityAction>
                            </Catch>
                            <Catch x:TypeArguments="s:NotSupportedException" sap2010:WorkflowViewState.IdRef="Catch`1_3">
                              <ActivityAction x:TypeArguments="s:NotSupportedException">
                                <ActivityAction.Argument>
                                  <DelegateInArgument x:TypeArguments="s:NotSupportedException" Name="exception" />
                                </ActivityAction.Argument>
                                <ui:LogMessage DisplayName="Log message" sap2010:WorkflowViewState.IdRef="LogMessage_4" Level="Warn" Message="[&quot;[PROCESS.KILL] Failed to kill process &quot; + process_name + &quot; &quot; + exception.Message]" />
                              </ActivityAction>
                            </Catch>
                          </TryCatch.Catches>
                        </TryCatch>
                      </Sequence>
                    </If.Then>
                  </If>
                </ActivityAction>
              </ui:ForEach.Body>
            </ui:ForEach>
          </Sequence>
        </ActivityAction>
      </ui:ForEach.Body>
    </ui:ForEach>
    <sads:DebugSymbol.Symbol></sads:DebugSymbol.Symbol>
  </Sequence>
