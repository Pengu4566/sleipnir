  <Sequence sap2010:Annotation.AnnotationText="Writes standard transaction report. Writes a timestamped exceldocument whose name is the current date. &#xA;&#xA;Arguments:&#xA;in_str_pathToReportFolder&#xA;in_datatable_reportTable" DisplayName="Write report" sap2010:WorkflowViewState.IdRef="Sequence_34">
    <Sequence.Variables>
      <Variable x:TypeArguments="x:Boolean" Name="bool_reportExists" />
      <Variable x:TypeArguments="x:String" Name="str_tmpReportLocation" />
    </Sequence.Variables>
    <Assign DisplayName="Set temp report location" sap2010:WorkflowViewState.IdRef="Assign_44">
      <Assign.To>
        <OutArgument x:TypeArguments="x:String">[str_tmpReportLocation]</OutArgument>
      </Assign.To>
      <Assign.Value>
        <InArgument x:TypeArguments="x:String">[in_str_pathToReportFolder + "\" + Today.ToString(in_str_datetimeFormat) + ".xlsx"]</InArgument>
      </Assign.Value>
    </Assign>
