    <ui:PathExists DisplayName="Path exists" Exists="[bool_reportExists]" sap2010:WorkflowViewState.IdRef="PathExists_1" Path="[str_tmpReportLocation]" PathType="File" />
    <If Condition="[bool_reportExists = True]" DisplayName="If - Report exists." sap2010:WorkflowViewState.IdRef="If_15">
      <If.Then>
        <ui:AppendRange DataTable="[in_datatable_reportTable]" DisplayName="Append range" sap2010:WorkflowViewState.IdRef="AppendRange_1" SheetName="Sheet1" WorkbookPath="[str_tmpReportLocation]" />
